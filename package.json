{
  "name": "lamasign-components-library",
  "version": "0.1.0",
  "description": "VueJS UI Components made by Malo Lebrin",
  "author": "Malo Lebrin",
  "license": "Malo Lebrin Copyright",
  "private": true,
  "files": [
    "lib/*"
  ],
  "types": "",
  "main": "dist/BeRightComponentLibrary.umd.min.js",
  "unpkg": "dist/BeRightComponentLibrary.umd.min.js",
  "jsdelivr": "dist/BeRightComponentLibrary.umd.min.js",
  "scripts": {
    "build": "vue-cli-service build --target lib --name BeRightComponentLibrary src/index.ts",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "dev": "vue-cli-service serve",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test:unit",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "@tailwindcss/postcss7-compat": "^2.0.2",
    "@vue/composition-api": "^1.1.5",
    "autoprefixer": "^9",
    "core-js": "^3.6.5",
    "decimal.js-light": "^2.5.1",
    "postcss": "^7",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.0.2",
    "vue": "^2.6.11",
    "vue-click-outside": "^1.1.0"
  },
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "@vue/test-utils": "^1.2.2",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "typescript": "~4.1.5",
    "vue-cli-plugin-tailwind": "~2.0.6",
    "vue-jest": "^3.0.7",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/typescript"
    ],
    "parserOptions": {
      "parser": "@typescript-eslint/parser"
    },
    "rules": {},
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "keywords": [
    "vue",
    "vuejs",
    "jest",
    "typescript",
    "tailwindcss",
    "lamasign-components-library",
    "components",
    "library"
  ]
}